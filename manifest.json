{
  "manifest_version": 2,

  "name": "Doublespeak",
  "description": "This extension replaces political doublespeak with plain English.",
  "version": "0.1",
  "permissions": [
    "tabs", "http://*/*"
  ],

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["doublespeak.js"],
      "run_at": "document_end"
    }
  ]
}